    "moment": "^2.23.0",
    "naf-core": "0.0.27",
    "qs": "^6.6.0",
    "urijs": "^1.19.1",
    "vue": "^2.5.21",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.2",
    "@vue/cli-plugin-eslint": "^3.2.2",
    "@vue/cli-service": "^3.2.2",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
